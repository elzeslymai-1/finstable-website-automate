{
  "name": "finstable-website-automate-testing",
  "version": "1.0",
  "main": "cypress/support/commands.js",
  "keywords": [
    "Cypress",
    "Testing",
    "EthersJS",
    "Smart Contracts",
    "Blockchain",
    "MetaMask",
    "Utils"
  ],
  "standard": {
    "env": [
      "cypress/globals"
    ],
    "parser": "babel-eslint",
    "plugins": [
